            "types": "./dist/plugins/jsx-remove-attributes.d.ts",
            "default": "./dist/plugins/jsx-remove-attributes.js"
        },
        "./plugin/license": {
            "types": "./dist/plugins/license.d.ts",
            "default": "./dist/plugins/license.js"
