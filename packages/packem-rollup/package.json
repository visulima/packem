            "default": "./dist/plugins/source-maps.js"
        },
        "./plugin/url": {
            "types": "./dist/plugins/url.d.ts",
            "default": "./dist/plugins/url.js"
        },
        "./plugin/json": {
            "types": "./dist/plugins/json.d.ts",
            "default": "./dist/plugins/json.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "CHANGELOG.md",
        "README.md",
        "dist"
    ],
    "scripts": {
        "build": "./node_modules/.bin/packem build --development",
        "build:prod": "./node_modules/.bin/packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint . --cache --concurrency auto",
        "lint:eslint:fix": "eslint . --cache --concurrency auto --fix,",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@babel/parser": "catalog:prod",
        "@rollup/plugin-alias": "catalog:build",
