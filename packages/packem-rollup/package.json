{
    "name": "@visulima/packem-rollup",
    "version": "1.0.0-alpha.5",
    "description": "Rollup plugins for packem",
    "keywords": [
        "visulima",
        "packem-rollup"
    ],
    "homepage": "https://github.com/visulima/packem/tree/main/packages/packem-rollup",
    "bugs": {
        "url": "https://github.com/visulima/packem/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/packem.git",
        "directory": "packages/packem-rollup"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "./esbuild": {
            "types": "./dist/plugins/esbuild/index.d.mts",
            "default": "./dist/plugins/esbuild/index.mjs"
        },
        "./swc": {
            "types": "./dist/plugins/swc/index.d.mts",
            "default": "./dist/plugins/swc/index.mjs"
        },
        "./typescript": {
            "types": "./dist/plugins/typescript/index.d.mts",
            "default": "./dist/plugins/typescript/index.mjs"
        },
        "./oxc": {
            "types": "./dist/plugins/oxc/index.d.mts",
            "default": "./dist/plugins/oxc/index.mjs"
        },
        "./sucrase": {
            "types": "./dist/plugins/sucrase/index.d.mts",
            "default": "./dist/plugins/sucrase/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "CHANGELOG.md",
        "README.md",
        "dist"
    ],
    "scripts": {
        "build": "../../node_modules/.bin/packem build --development",
        "build:prod": "../../node_modules/.bin/packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@babel/parser": "7.28.0",
        "@rollup/plugin-alias": "5.1.1",
        "@rollup/plugin-commonjs": "28.0.6",
        "@rollup/plugin-dynamic-import-vars": "2.1.5",
        "@rollup/plugin-inject": "5.0.5",
        "@rollup/plugin-json": "6.1.0",
        "@rollup/plugin-node-resolve": "16.0.1",
        "@rollup/plugin-replace": "6.0.2",
        "@rollup/plugin-wasm": "6.2.2",
        "@rollup/pluginutils": "5.2.0",
        "@visulima/find-cache-dir": "1.0.31",
        "@visulima/fs": "3.1.5",
        "@visulima/package": "3.5.8",
        "@visulima/packem-share": "1.0.0-alpha.5",
        "@visulima/path": "1.4.0",
        "@visulima/source-map": "1.0.20",
        "es-module-lexer": "1.7.0",
        "glob-parent": "6.0.2",
        "magic-string": "0.30.17",
        "mlly": "1.7.4",
        "oxc-parser": "0.81.0",
        "oxc-resolver": "11.6.1",
        "rollup-plugin-dts": "6.2.1",
        "rollup-plugin-license": "3.6.0",
        "rollup-plugin-polyfill-node": "0.13.0",
        "rollup-plugin-pure": "^0.4.0",
        "rollup-plugin-visualizer": "6.0.3",
        "semver": "7.7.2",
        "tinyglobby": "0.2.14"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.2.23",
        "@anolilab/semantic-release-pnpm": "^2.0.3",
        "@anolilab/semantic-release-preset": "^11.0.3",
        "@babel/core": "^7.28.0",
        "@oxc-parser/binding-linux-x64-gnu": "0.81.0",
        "@oxc-project/types": "0.81.0",
        "@oxc-resolver/binding-linux-x64-gnu": "11.6.1",
        "@rushstack/eslint-plugin-security": "^0.11.0",
        "@secretlint/secretlint-rule-preset-recommend": "^11.0.1",
        "@swc/core": "^1.13.3",
        "@swc/types": "0.1.24",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/estree": "^1.0.8",
        "@types/glob-parent": "^5.1.3",
        "@types/is-glob": "^4.0.4",
        "@types/node": "24.2.1",
        "@types/semver": "^7.7.0",
        "@visulima/pail": "2.1.25",
        "@visulima/tsconfig": "^1.1.19",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "browserslist": "4.25.2",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "esbuild": "^0.25.8",
        "eslint": "^9.33.0",
        "estree-walker": "^3.0.3",
        "hookable": "5.5.3",
        "jiti": "2.5.1",
        "mime": "4.0.7",
        "oxc-transform": "^0.81.0",
        "p-queue": "^8.1.0",
        "rimraf": "^6.0.1",
        "rollup": "4.46.2",
        "secretlint": "11.0.1",
        "semantic-release": "^24.2.7",
        "sucrase": "^3.35.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "@swc/core": "*",
        "esbuild": "*",
        "oxc-transform": "*",
        "rollup": ">=4",
        "sucrase": "*",
        "typescript": "*"
    },
    "peerDependenciesMeta": {
        "@swc/core": {
            "optional": true
        },
        "esbuild": {
            "optional": true
        },
        "oxc-transform": {
            "optional": true
        },
        "p-queue": {
            "optional": true
        },
        "sucrase": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.* <=24.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
