        "./plugin/esm-shim-cjs-syntax": {
            "types": "./dist/plugins/esm-shim-cjs-syntax.d.ts",
            "default": "./dist/plugins/esm-shim-cjs-syntax.js"
        },
        "./plugin/fix-dts-default-cjs-exports": {
            "types": "./dist/plugins/fix-dts-default-cjs-exports.d.ts",
