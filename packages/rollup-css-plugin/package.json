{
    "name": "@visulima/rollup-css-plugin",
    "version": "1.0.0-alpha.10",
    "description": "CSS processing plugin for packem",
    "keywords": [
        "visulima",
        "rollup-css-plugin",
        "css",
        "postcss",
        "sass",
        "less",
        "stylus",
        "css-modules",
        "lightningcss",
        "cssnano",
        "tailwind",
        "tailwindcss",
        "rollup-plugin"
    ],
    "homepage": "https://github.com/visulima/packem/tree/main/packages/rollup-css-plugin",
    "bugs": {
        "url": "https://github.com/visulima/packem/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/packem.git",
        "directory": "packages/packem-css"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "./loader/sourcemap": {
            "types": "./dist/loaders/sourcemap.d.mts",
            "default": "./dist/loaders/sourcemap.mjs"
        },
        "./loader/less": {
            "types": "./dist/loaders/less/index.d.mts",
            "default": "./dist/loaders/less/index.mjs"
        },
        "./loader/postcss": {
            "types": "./dist/loaders/postcss/index.d.mts",
            "default": "./dist/loaders/postcss/index.mjs"
        },
        "./loader/sass": {
            "types": "./dist/loaders/sass/index.d.mts",
            "default": "./dist/loaders/sass/index.mjs"
        },
        "./loader/stylus": {
            "types": "./dist/loaders/stylus/index.d.mts",
            "default": "./dist/loaders/stylus/index.mjs"
        },
        "./loader/lightningcss": {
            "types": "./dist/loaders/lightningcss.d.mts",
            "default": "./dist/loaders/lightningcss.mjs"
        },
        "./loader/tailwindcss": {
            "types": "./dist/loaders/tailwindcss-oxide.d.mts",
            "default": "./dist/loaders/tailwindcss-oxide.mjs"
        },
        "./minifier/cssnano": {
            "types": "./dist/minifiers/cssnano.d.mts",
            "default": "./dist/minifiers/cssnano.mjs"
        },
        "./minifier/lightningcss": {
            "types": "./dist/minifiers/lightningcss.d.mts",
            "default": "./dist/minifiers/lightningcss.mjs"
        },
        "./utils": {
            "types": "./dist/utils/index.d.mts",
            "default": "./dist/utils/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "CHANGELOG.md",
        "README.md",
        "dist"
    ],
    "scripts": {
        "build": "../../node_modules/.bin/packem build --development",
        "build:prod": "../../node_modules/.bin/packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.2.0",
        "@visulima/fs": "^3.1.5",
        "@visulima/package": "^3.5.8",
        "@visulima/packem-share": "1.0.0-alpha.8",
        "@visulima/path": "^1.4.0",
        "mlly": "^1.8.0",
        "oxc-resolver": "^11.6.2",
        "p-queue": "^8.1.0",
        "source-map-js": "^1.2.1"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.2.23",
        "@anolilab/semantic-release-pnpm": "2.0.3",
        "@anolilab/semantic-release-preset": "^11.0.3",
        "@csstools/css-parser-algorithms": "3.0.5",
