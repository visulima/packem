{
    "name": "@visulima/rollup-css-plugin",
    "version": "0.0.0",
    "description": "CSS processing plugin for packem",
    "keywords": [
        "visulima",
        "rollup-css-plugin",
        "css",
        "postcss",
        "sass",
        "less",
        "stylus",
        "css-modules",
        "lightningcss",
        "cssnano",
        "rollup-plugin"
    ],
    "homepage": "https://github.com/visulima/packem/tree/main/packages/rollup-css-plugin",
    "bugs": {
        "url": "https://github.com/visulima/packem/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/packem.git",
        "directory": "packages/packem-css"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "browser": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ]
        },
        ">=5.0": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:package-json": "publint --strict",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.2.0",
        "@visulima/fs": "^3.1.5",
        "@visulima/path": "^1.0.0",
        "mlly": "^1.7.4",
        "oxc-resolver": "^11.2.1",
        "p-queue": "^8.1.0",
        "source-map-js": "^1.2.1"
    },
    "devDependencies": {
        "@visulima/pail": "^2.1.25",
        "@visulima/packem-share": "workspace:*",
        "postcss": ">=8.4.47",
        "postcss-load-config": ">=6.0.1",
        "postcss-modules-extract-imports": ">=3.1.0",
        "postcss-modules-local-by-default": ">=4.0.5",
        "postcss-modules-scope": ">=3.2.0",
        "postcss-modules-values": ">=4.0.0",
        "postcss-value-parser": ">=4.2.0",
        "node-sass": ">=9.0.0",
        "@anolilab/eslint-config": "^16.2.13",
        "@anolilab/semantic-release-pnpm": "2.0.0",
        "@anolilab/semantic-release-preset": "^11.0.0",
        "@babel/core": "^7.27.4",
        "@rushstack/eslint-plugin-security": "^0.10.0",
        "@secretlint/secretlint-rule-preset-recommend": "^10.1.1",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "24.0.4",
        "@visulima/packem": "^1.28.2",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "conventional-changelog-conventionalcommits": "9.0.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.25.5",
        "eslint": "^9.29.0",
        "rimraf": "^6.0.1",
        "rollup": "^4.44.0",
        "secretlint": "10.1.1",
        "sass": ">=1.43.4",
        "sass-embedded": ">=1.79.4",
        "stylus": ">=0.63.0",
        "semantic-release": "^24.2.5",
        "typescript": "^5.8.3",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "postcss": "^8.0.0",
        "postcss-modules-extract-imports": ">=3.1.0",
        "postcss-modules-local-by-default": ">=4.0.5",
        "postcss-modules-scope": ">=3.2.0",
        "postcss-modules-values": ">=4.0.0",
        "postcss-value-parser": ">=4.2.0",
        "postcss-load-config": "^6.0.1",
        "sass": "^1.0.0",
        "sass-embedded": ">=1.79.4",
        "stylus": "^0.60.0",
        "cssnano": "^7.0.0",
        "less": "^4.0.0",
        "lightningcss": "^1.0.0",
        "rollup": "^4.21.2",
        "icss-utils": ">=5.1.0",
        "node-sass": ">=9.0.0"
    },
    "peerDependenciesMeta": {
        "icss-utils": {
            "optional": true
        },
        "cssnano": {
            "optional": true
        },
        "node-sass": {
            "optional": true
        },
        "less": {
            "optional": true
        },
        "lightningcss": {
            "optional": true
        },
        "postcss": {
            "optional": true
        },
        "postcss-load-config": {
            "optional": true
        },
        "postcss-modules-extract-imports": {
            "optional": true
        },
        "postcss-modules-local-by-default": {
            "optional": true
        },
        "postcss-modules-scope": {
            "optional": true
        },
        "postcss-modules-values": {
            "optional": true
        },
        "postcss-value-parser": {
            "optional": true
        },
        "sass": {
            "optional": true
        },
        "sass-embedded": {
            "optional": true
        },
        "stylus": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.* <=24.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
