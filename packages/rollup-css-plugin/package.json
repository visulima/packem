        "rimraf": "^6.0.1",
        "rollup": "^4.48.1",
        "sass": "^1.90.0",
        "sass-embedded": "^1.90.0",
        "secretlint": "^11.2.0",
        "semantic-release": "^24.2.7",
        "stylus": "^0.64.0",
        "sugarss": "5.0.1",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
