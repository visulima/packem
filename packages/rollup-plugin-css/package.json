{
    "name": "@visulima/rollup-plugin-css",
    "version": "1.0.0-alpha.7",
    "description": "CSS processing plugin for packem",
    "keywords": [
        "visulima",
        "rollup-plugin-css",
        "css",
        "postcss",
        "sass",
        "less",
        "stylus",
        "css-modules",
        "lightningcss",
        "cssnano",
        "tailwind",
        "tailwindcss",
        "rollup-plugin"
    ],
    "homepage": "https://github.com/visulima/packem/tree/main/packages/rollup-plugin-css",
    "bugs": {
        "url": "https://github.com/visulima/packem/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/packem.git",
        "directory": "packages/rollup-plugin-css"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./loader/sourcemap": {
            "types": "./dist/loaders/sourcemap.d.ts",
            "default": "./dist/loaders/sourcemap.js"
        },
        "./loader/less": {
            "types": "./dist/loaders/less/index.d.ts",
            "default": "./dist/loaders/less/index.js"
        },
        "./loader/postcss": {
            "types": "./dist/loaders/postcss/index.d.ts",
            "default": "./dist/loaders/postcss/index.js"
        },
        "./loader/sass": {
            "types": "./dist/loaders/sass/index.d.ts",
            "default": "./dist/loaders/sass/index.js"
        },
        "./loader/stylus": {
            "types": "./dist/loaders/stylus/index.d.ts",
            "default": "./dist/loaders/stylus/index.js"
        },
        "./loader/lightningcss": {
            "types": "./dist/loaders/lightningcss.d.ts",
            "default": "./dist/loaders/lightningcss.js"
        },
        "./loader/tailwindcss": {
            "types": "./dist/loaders/tailwindcss-oxide.d.ts",
            "default": "./dist/loaders/tailwindcss-oxide.js"
        },
        "./minifier/cssnano": {
            "types": "./dist/minifiers/cssnano.d.ts",
            "default": "./dist/minifiers/cssnano.js"
        },
        "./minifier/lightningcss": {
            "types": "./dist/minifiers/lightningcss.d.ts",
            "default": "./dist/minifiers/lightningcss.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "default": "./dist/utils/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "CHANGELOG.md",
        "README.md",
        "dist"
    ],
    "scripts": {
        "build": "./node_modules/.bin/packem build --development",
        "build:prod": "./node_modules/.bin/packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint . --cache --concurrency auto",
        "lint:eslint:fix": "eslint . --cache --concurrency auto --fix,",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@rollup/pluginutils": "catalog:build",
        "@visulima/fs": "catalog:prod",
        "@visulima/package": "catalog:prod",
        "@visulima/packem-share": "1.0.0-alpha.13",
        "@visulima/path": "catalog:node",
        "mlly": "catalog:prod",
        "oxc-resolver": "catalog:node",
        "p-queue": "catalog:prod",
        "source-map-js": "catalog:prod"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "catalog:lint",
        "@anolilab/semantic-release-pnpm": "catalog:cli",
        "@anolilab/semantic-release-preset": "catalog:cli",
        "@arethetypeswrong/cli": "catalog:dev",
        "@csstools/css-parser-algorithms": "catalog:style",
