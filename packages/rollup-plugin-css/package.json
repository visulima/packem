        "postcss-modules-values": "catalog:style",
        "postcss-value-parser": "catalog:style",
        "publint": "catalog:lint",
        "rimraf": "catalog:node",
        "rollup": "catalog:build",
        "sass": "catalog:style",
        "sass-embedded": "catalog:style",
        "secretlint": "catalog:lint",
        "semantic-release": "catalog:cli",
        "stylus": "catalog:dev",
        "sugarss": "catalog:dev",
        "typescript": "catalog:tsc",
        "vitest": "catalog:test"
    },
    "peerDependencies": {
        "@csstools/css-parser-algorithms": ">=3.0.5",
        "@csstools/css-tokenizer": ">=3.0.4",
        "@csstools/postcss-slow-plugins": ">=2.0.0",
        "@tailwindcss/node": ">=4.1.12",
        "@tailwindcss/oxide": ">=4.1.12",
        "@visulima/css-style-inject": "1.0.0-alpha.9",
        "cssnano": ">=7.0.0",
        "icss-utils": ">=5.1.0",
        "less": ">=4.0.0",
        "lightningcss": ">=1.0.0",
