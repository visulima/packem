        "@csstools/postcss-slow-plugins": "catalog:style",
        "@rollup/plugin-inject": "catalog:build",
        "@secretlint/secretlint-rule-preset-recommend": "catalog:lint",
        "@tailwindcss/node": "catalog:style",
        "@tailwindcss/oxide": "catalog:style",
        "@total-typescript/ts-reset": "catalog:tsc",
        "@types/icss-utils": "catalog:types",
        "@types/less": "catalog:types",
        "@types/node": "catalog:types",
        "@types/postcss-modules-local-by-default": "catalog:types",
        "@types/postcss-modules-scope": "catalog:types",
        "@types/stylus": "catalog:types",
        "@types/uglifycss": "catalog:types",
        "@visulima/css-style-inject": "1.0.0-alpha.9",
        "@visulima/packem": "catalog:dev",
        "@vitest/coverage-v8": "catalog:test",
        "@vitest/ui": "catalog:test",
        "conventional-changelog-conventionalcommits": "catalog:dev",
        "cssnano": "catalog:style",
        "eslint": "catalog:lint",
        "icss-utils": "catalog:style",
        "less": "catalog:style",
        "lightningcss": "catalog:style",
        "mime": "catalog:dev",
        "postcss": "catalog:style",
        "postcss-load-config": "catalog:style",
