{
    "name": "@visulima/typedoc-plugins",
    "version": "0.0.0",
    "description": "Various TypeDoc plugins.",
    "keywords": [
        "visulima",
        "typedoc-plugins"
    ],
    "homepage": "https://github.com/visulima/packem/tree/main/packages/typedoc-plugins",
    "bugs": {
        "url": "https://github.com/visulima/packem/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/visulima/packem.git",
        "directory": "packages/typedoc-plugins"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        },
        "./event-inheritance-fixer": {
            "require": {
                "types": "./dist/event-inheritance-fixer.d.cts",
                "default": "./dist/event-inheritance-fixer.cjs"
            },
            "import": {
                "types": "./dist/event-inheritance-fixer.d.mts",
                "default": "./dist/event-inheritance-fixer.mjs"
            }
        },
        "./event-param-fixer": {
            "require": {
                "types": "./dist/event-param-fixer.d.cts",
                "default": "./dist/event-param-fixer.cjs"
            },
            "import": {
                "types": "./dist/event-param-fixer.d.mts",
                "default": "./dist/event-param-fixer.mjs"
            }
        },
        "./interface-augmentation-fixer": {
            "require": {
                "types": "./dist/interface-augmentation-fixer.d.cts",
                "default": "./dist/interface-augmentation-fixer.cjs"
            },
            "import": {
                "types": "./dist/interface-augmentation-fixer.d.mts",
                "default": "./dist/interface-augmentation-fixer.mjs"
            }
        },
        "./module-fixer": {
            "require": {
                "types": "./dist/module-fixer.d.cts",
                "default": "./dist/module-fixer.cjs"
            },
            "import": {
                "types": "./dist/module-fixer.d.mts",
                "default": "./dist/module-fixer.mjs"
            }
        },
        "./purge-private-api-docs": {
            "require": {
                "types": "./dist/purge-private-api-docs.d.cts",
                "default": "./dist/purge-private-api-docs.cjs"
            },
            "import": {
                "types": "./dist/purge-private-api-docs.d.mts",
                "default": "./dist/purge-private-api-docs.mjs"
            }
        },
        "./symbol-fixer": {
            "require": {
                "types": "./dist/symbol-fixer.d.cts",
                "default": "./dist/symbol-fixer.cjs"
            },
            "import": {
                "types": "./dist/symbol-fixer.d.mts",
                "default": "./dist/symbol-fixer.mjs"
            }
        },
        "./tag-error": {
            "require": {
                "types": "./dist/tag-error.d.cts",
                "default": "./dist/tag-error.cjs"
            },
            "import": {
                "types": "./dist/tag-error.d.mts",
                "default": "./dist/tag-error.mjs"
            }
        },
        "./tag-event": {
            "require": {
                "types": "./dist/tag-event.d.cts",
                "default": "./dist/tag-event.cjs"
            },
            "import": {
                "types": "./dist/tag-event.d.mts",
                "default": "./dist/tag-event.mjs"
            }
        },
        "./tag-observable": {
            "require": {
                "types": "./dist/tag-observable.d.cts",
                "default": "./dist/tag-observable.cjs"
            },
            "import": {
                "types": "./dist/tag-observable.d.mts",
                "default": "./dist/tag-observable.mjs"
            }
        },
        "./utils": {
            "require": {
                "types": "./dist/utils.d.cts",
                "default": "./dist/utils.cjs"
            },
            "import": {
                "types": "./dist/utils.d.mts",
                "default": "./dist/utils.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "browser": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        ">=5.5": {
            ".": [
                "./dist/index.d.ts"
            ],
            "event-inheritance-fixer": [
                "./dist/event-inheritance-fixer.d.ts"
            ],
            "event-param-fixer": [
                "./dist/event-param-fixer.d.ts"
            ],
            "interface-augmentation-fixer": [
                "./dist/interface-augmentation-fixer.d.ts"
            ],
            "module-fixer": [
                "./dist/module-fixer.d.ts"
            ],
            "purge-private-api-docs": [
                "./dist/purge-private-api-docs.d.ts"
            ],
            "symbol-fixer": [
                "./dist/symbol-fixer.d.ts"
            ],
            "tag-error": [
                "./dist/tag-error.d.ts"
            ],
            "tag-event": [
                "./dist/tag-event.d.ts"
            ],
            "tag-observable": [
                "./dist/tag-observable.d.ts"
            ],
            "utils": [
                "./dist/utils.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development packem build",
        "build:prod": "cross-env NODE_ENV=production packem build",
        "clean": "rimraf node_modules dist .eslintcache",
        "dev": "pnpm run build --watch",
        "lint:eslint": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs --fix",
        "lint:package-json": "publint --strict",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:types": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/path": "^1.1.2"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^15.0.3",
        "@anolilab/prettier-config": "^5.0.14",
        "@anolilab/semantic-release-pnpm": "1.1.5",
        "@anolilab/semantic-release-preset": "^9.0.2",
        "@babel/core": "^7.26.0",
        "@rushstack/eslint-plugin-security": "^0.8.3",
        "@secretlint/secretlint-rule-preset-recommend": "^9.0.0",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "18.19.59",
        "@visulima/packem": "1.10.0",
        "@vitest/coverage-v8": "^2.1.8",
        "@vitest/ui": "^2.1.8",
        "conventional-changelog-conventionalcommits": "8.0.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.24.0",
        "eslint": "8.57.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
        "eslint-plugin-mdx": "^3.1.5",
        "eslint-plugin-vitest": "0.4.1",
        "eslint-plugin-vitest-globals": "^1.5.0",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",
        "secretlint": "9.0.0",
        "semantic-release": "^24.2.0",
        "tinyglobby": "0.2.10",
        "typedoc": "^0.27.5",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    },
    "peerDependencies": {
        "typedoc": ">=0.27"
    },
    "engines": {
        "node": ">=18.* <=22.*"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false,
                "etc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false,
            "info_on_disabling_etc_no_deprecated": false
        }
    }
}
